{
  "name": "gigabit/affilinet-php-sdk",
  "description": "affilinet PHP SDK, forked and extended from https://github.com/affilinet/productdata-php-sdk",
  "keywords": ["affilinet", "sdk"],
  "type": "library",
  "homepage": "https://github.com/gigabit-gmbh/affilinet-php-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Gotre / teraone.de",
      "homepage": "https://www.teraone.de"
    },
    {
      "name": "Thomas Helmrich / gigabit.de",
      "homepage": "https://www.gigabit.de"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "ext-json" : "*",
    "guzzlehttp/guzzle": "~6.0",
    "monolog/monolog": "^1.20",
    "psr/http-message": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "vladahejda/phpunit-assert-exception": "^1.0",
    "apigen/apigen": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Affilinet\\": "src/Affilinet"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Affilinet\\Tests\\": "Tests/"
    }
  }
}
